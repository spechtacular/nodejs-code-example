			var usersTable = $('#usersTable').DataTable({
				  paging: true,
				  lengthMenu: false,
				  info: true,
				  searching: true,
				  responsive: true,
				  "order": [[ 1, 'asc' ]],
				  columnDefs: [
						{ responsivePriority: 1, targets: 0 },
						{ "orderable": false, "targets": -1 },
						{ "orderable": false, "targets": -2 }
					 ]        
			});
	
			usersTable.on( 'responsive-display', function ( e, datatable, row, showHide, update ) { //Triggers when a details row is expanded
				if (showHide == true) {
					var rowID = $(row.node()).attr("data-row-id");
					$($(".rolesForUser" + rowID)[1]).attr("src", $(row.node()).attr("data-roles-url"));
					$($(".facilitiesForUser" + rowID)[1]).attr("src", $(row.node()).attr("data-facilities-url"));
					$(".rolesForUser" + rowID).on("load", function () { //Load iframe content for roles and facilities
						$(".rolesForUser" + rowID).show();
						$(".rolesForUser" + rowID).parent().find("i").hide();
					});
					$(".facilitiesForUser" + rowID).on("load", function() {
						$(".facilitiesForUser" + rowID).show();
						$(".facilitiesForUser" + rowID).parent().find("i").hide();
					});
				}
			});
	
			usersTable.on( 'draw.dt', function () { //Triggers on redraw/search/navigation/filter
				usersTable.rows().eq(0).each( function ( i, e ) {
						  var row = usersTable.row(i);
						  var tr = row.node();

						  if (row.child.isShown() ) { //Hide child rows when navigating
							  $(tr).removeClass('parent');
							  row.child.hide();
						  }
					});
			} );
